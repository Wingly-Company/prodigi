{
    "name": "wingly/prodigi",
    "description": "A wrapper for Prodigi Print API",
    "authors": [
        {
            "name": "Dimitris Karapanos",
            "email": "dimitris@wingly.io",
            "homepage": "https://www.wingly.io",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/Wingly-Company/prodigi",
    "keywords": [
        "Laravel",
        "Prodigi"
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "guzzlehttp/guzzle": "^7.9.3",
        "illuminate/support": "^10.48.28|^11.44.4|^12.10.1",
        "spatie/data-transfer-object": "^3.9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.45|^11.5.18",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^8.35.0|^9.13.0|^10.2.1",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "autoload": {
        "psr-4": {
            "Wingly\\Prodigi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wingly\\Prodigi\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wingly\\Prodigi\\ProdigiServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
